target C {
    scheduler: FS
}

main reactor {
    logical action a(1 sec);
    timer t(2 sec, 1 sec);
    reaction(startup) -> a {=
        printf("Reaction 1 triggered by startup at %lld\n", lf_time_logical());
        lf_schedule(a, 0);
    =}
    reaction(a) {=
        printf("Reaction 2 triggered by logical action at %lld\n", lf_time_logical());
    =}
    reaction(t) {=
        printf("Reaction 3 triggered by timer at %lld\n", lf_time_logical());
    =}
}